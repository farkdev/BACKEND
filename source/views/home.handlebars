
<div class="product-list">
  {{#each lista}}
  <div class="product-item">
    
    <div class="product-details">
      
      <h4>{{this.titulo}}</h4>
      <p class="product-price"> ID: {{this.id}}</p>
      <p class="product-description">{{this.descripcion}}</p>
      <p class="product-price">$ {{this.precio}}</p>

    
    </div>
  </div>
  {{/each}}
</div>
